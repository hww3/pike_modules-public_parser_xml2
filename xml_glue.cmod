#include "util.h"
#include "module.h"

void pike_module_init()
{
  INIT
  pike_init_xml2_node();
  pike_init_xml2_constants();
  pike_init_xml2_sax();
  pike_init_xml2_stylesheet();
  pike_init_xml2_main();
}

void pike_module_exit()
{
  EXIT
  pike_exit_xml2_constants();
  pike_exit_xml2_node();
  pike_exit_xml2_sax();
  pike_exit_xml2_stylesheet();
  pike_exit_xml2_main();
}
