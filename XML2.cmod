 *!  
 *!  sets a new attribute, overwriting any existing value.
 *!  if @[value] is zero, the attribute will be removed from the node.
 */
PIKEFUN object set_attribute(string name, string value, string ns)
{
  struct object * o;
  xmlAttrPtr attr;
  xmlNsPtr ns;

  check_node_created();

  attr = xmlNewNsProp(MY_NODE, ns, (xmlChar *)name->str, (xmlChar *)value->str);

  if(attr == NULL)
  {
    push_int(0);
    return;
  }
  else
  {
    o = NEW_NODE();
    OBJ2_NODE(o)->object_data->node = THIS->object_data->node;
    NEW_NODE_REFS(o);
    push_object(o);
  }
}

