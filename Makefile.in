# $Id: Makefile.in,v 1.7 2005-04-07 01:18:00 hww3 Exp $
@make_variables@
VPATH=@srcdir@:@srcdir@/../..:../..
OBJS=xml_main.o
MODDIR=Public.pmod/Parser.pmod/
MODNAME=XML2
MODULE_PMOD_IN=
AUTODOC_SRC_IN=xml_main.c

CFLAGS=@XML2_CFLAGS@
LDFLAGS=@XML2_LIBS@

MODULE_LDFLAGS=@LDFLAGS@

CONFIG_HEADERS=@CONFIG_HEADERS@

@dynamic_module_makefile@
@dependencies@

release:
	cvs rtag -D now RELEASE_1_${MIN} pike_modules/Public_Parser_XML2
	cvs export -d Public_Parser_XML2-1.${MIN} -r RELEASE_1_${MIN} pike_modules/Public_Parser_XML2
	cd Public_Parser_XML2-1.${MIN} && \
	pike -x module --autoconf && \
	rm -rf autom4*
	gtar czvf Public_Parser_XML2-1.${MIN}.tar.gz Public_Parser_XML2-1.${MIN}

